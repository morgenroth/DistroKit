image @IMAGE@ {
    vfat {
        files = { @FILES@ }
        file linuximage { image = "@KERNEL@" }
	file barebox { image = "@BAREBOX@" }
	file barebox.env { image = "@BAREBOX_ENV@" }
        file wpan-at86rf233-rpi-b.dtb { image = "wpan-at86rf233-rpi-b.dtb" }
        file wpan-mrf24j40ma-rpi-b.dtb { image = "wpan-mrf24j40ma-rpi-b.dtb" }
    }
    name = boot-vfat
    size = 32M
}
